<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef>
    <defName>GG_UnnaturalHealing</defName>
    <label>UnnaturalHealing</label>
    <labelShortAdj>unnatural healer</labelShortAdj>
    <description>Carriers of this gene Use dark psychic influence to heal someone. The process will stop bleeding, restore blood loss, and can even heal significant injuries. However, it may also have unexpected side effects.</description>
    <iconPath>UI/Abilities/UnnaturalHealing</iconPath>
    <abilities>
      <li>UnnaturalHealing</li>
    </abilities>
    <descriptionHyperlinks>
      <AbilityDef>UnnaturalHealing</AbilityDef>
    </descriptionHyperlinks>
    <biostatMet>-3</biostatMet>
    <biostatCpx>3</biostatCpx>
    <minAgeActive>3</minAgeActive>
    <symbolPack>
      <suffixSymbols>
        <li><symbol>life</symbol></li>
        <li><symbol>clotter</symbol></li>
        <li><symbol>tender</symbol></li>
      </suffixSymbols>
    </symbolPack>
  </GeneDef>

